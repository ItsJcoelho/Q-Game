<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2B2D42">


    <Button
        android:id="@+id/button3"
        android:layout_width="338dp"
        android:layout_height="56dp"
        android:background="@drawable/button_awnser"
        android:gravity="center"
        android:paddingLeft="30dp"
        android:text="Button"
        android:textAlignment="textStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="252dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="338dp"
        android:layout_height="56dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/button_awnser"
        android:gravity="center"
        android:paddingLeft="30dp"
        android:text="Button"
        android:textAlignment="textStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <Button
        android:id="@+id/button6"
        android:layout_width="338dp"
        android:layout_height="56dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/button_awnser"
        android:gravity="center"
        android:paddingLeft="30dp"
        android:text="Button"
        android:textAlignment="textStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <Button
        android:id="@+id/button4"
        android:layout_width="338dp"
        android:layout_height="56dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/button_awnser"
        android:gravity="center"
        android:paddingLeft="30dp"
        android:text="Button"
        android:textAlignment="textStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Q-QUIZ"
        android:fontFamily="@font/inriasans_bold"
        android:textSize="25dp"
        android:textColor="#fff"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="43dp" />

    <Button
        android:id="@+id/go"
        android:layout_width="138dp"
        android:layout_height="38dp"
        android:layout_gravity="start"
        android:layout_marginBottom="36dp"
        android:background="@drawable/button_link"
        android:enabled="false"
        android:text="Desistir"
        android:textColor="@color/colorWhite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="141dp"
        android:layout_height="44dp"
        android:layout_gravity="start"
        android:layout_marginStart="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/button_auth"
        android:enabled="false"
        android:fontFamily="@font/inriasans_bold"
        android:text="Seguinte"
        android:textColor="@color/colorWhite"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/password"
        app:layout_constraintVertical_bias="0.884" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inriasans_regular"
        android:text="pergunta?"
        android:textColor="#fff"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="132dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inriasans_regular"
        android:text="01/15"
        android:textColor="#999999"
        android:textSize="22dp"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="88dp" />

</androidx.constraintlayout.widget.ConstraintLayout>